{
    "name": "DevX Trader",
    "version": "1.0.0",
    "description": "trading bot",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --poll src/index.ts",
        "build": "rimraf ./build && tsc",
        "start": "npm run build && node -r dotenv/config build/index.js",

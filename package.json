{
    "name": "tradovate",
    "version": "1.0.3",
    "description": "Typescript Tradovate package to quickly test and deploy trading strategies on Tradovate.",
    "scripts": {
        "build": "rimraf ./dist && tsc",
        "push": "yarn build && yarn publish",
        "lint": "npx eslint . --fix",
        "test": "npx jest --verbose",
        "coverage": "yarn test --coverage",
        "coveralls": "npx coveralls < ./coverage/lcov.info"
    },
    "author": "Colby Gilbert",
    "license": "MIT",
    "keywords": [
        "tradovate",
        "tradovate api",
        "typescript",
        "futures",
        "trading",
        "automated"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cgilly2fast/tradovate-typescript"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node"
    },
    "main": "dist/index.js",
    "files": [
        "dist/",
        "LICENSE",
        "README.md",
        "package.json"
    ],
    "types": "./dist/index.d.ts",
    "dependencies": {
        "axios": "^1.7.2",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@tsconfig/docusaurus": "^2.0.0",
        "@types/dotenv": "^8.2.0",
        "@types/jest": "^29.5.4",
        "@types/node": "^20.14.11",
        "@types/ws": "^8.5.5",
        "@typescript-eslint/eslint-plugin": "^7.16.1",
        "@typescript-eslint/parser": "^7.16.1",
        "coveralls": "^3.1.1",
        "dotenv": "^16.4.5",
        "eslint": "^9.7.0",
        "jest": "^29.7.0",
        "nyc": "^17.0.0",
        "prettier": "3.3.3",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.3",
        "ts-node": "^10.2.0",
        "typescript": "^5.5.3"
    }
}

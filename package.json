        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "firebase-admin": "^11.10.1",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.3.0",
        "@types/ws": "^8.5.5",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "eslint": "^8.47.0",
        "prettier": "2.8.8",
        "rimraf": "^5.0.1",
        "ts-node-dev": "^2.0.0"
    }
}

{
    "name": "tradovate",
    "version": "0.5.1",
    "description": "Typescript Tradovate package to quickly test and deploy trading strategies on Tradovate.",
    "scripts": {
        "build": "rimraf ./dist && tsc",
        "lint": "npx eslint . --fix"
    },
    "author": "Colby Gilbert",
    "license": "MIT",
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src"
        ],
        "exclude": [
            "**/*.d.ts"
        ],
        "all": true
    },
    "keywords": [
        "tradovate",
        "tradovate api",
        "typescript",
        "futures",
        "trading",
        "automated"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cgilly2fast/tradovate-typescript"
    },
    "main": "dist/index.js",
    "files": [
        "dist/",
        "LICENSE",
        "README.md",
        "package.json"
    ],
    "types": "./dist/index.d.ts",
    "dependencies": {
        "axios": "^1.4.0",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@mxssfd/typedoc-theme": "^1.1.3",
        "@tsconfig/docusaurus": "^2.0.0",
        "@types/node": "^20.3.0",
        "@types/ws": "^8.5.5",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "concat-md": "^0.5.1",
        "eslint": "^8.47.0",
        "jsdoc": "^4.0.2",
        "prettier": "2.8.8",
        "rimraf": "^5.0.1",
        "showdown": "^2.1.0",
        "ts-node": "^10.2.0",
        "typedoc": "^0.25.1",
        "typedoc-plugin-markdown": "^4.0.0-next.20",
        "typescript": "^4.9.5"
    }
}
